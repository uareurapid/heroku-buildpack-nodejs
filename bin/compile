#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir> <env-dir>


echo "starting the database"
if [[ -f "$ENV_DIR/MONGODB_USERNAME" ]] && [[ -f "$ENV_DIR/MONGODB_PASSWORD" ]]; then
  #vars for db acces
  MONGODB_USERNAME=$(cat "$ENV_DIR/MONGODB_USERNAME")
  MONGODB_PASSWORD=$(cat "$ENV_DIR/MONGODB_PASSWORD")

  mongo ds137100.mlab.com:37100/heroku_h5n41fcw -u $MONGODB_USERNAME -p $MONGODB_PASSWORD
fi



#node server/app.js
